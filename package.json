{
  "name": "aigcstarter",
  "version": "0.0.1",
  "description": "aigc starter,webui,comfyui,Orillusion in",
  "main": "dist/main/index.cjs",
  "scripts": {
    "dev": "cross-env VITE_RUNTIME_ENV=github node scripts/watch.mjs",
    "preinstall": "cd packages/orillusion-editor && npm install",
    "prestart": "cd packages/orillusion-editor && npm run build && cd ../../ tsc",
    "start": "electron .",
    "prebuild": "node scripts/build.mjs",
    "build": "electron-builder",
    "build:mac-universal": "node scripts/build.mjs && electron-builder --universal",
    "postinstall": "node scripts/postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhoukang0807/aigcstarter.git"
  },
  "author": "weehel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhoukang0807/aigcstarter/issues"
  },
  "homepage": "https://github.com/zhoukang0807/aigcstarter#readme",
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "cross-env": "^7.0.3",
    "electron": "^27.1.2",
    "electron-builder": "^24.9.1",
    "electron-devtools-installer": "^3.2.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.2.0",
    "postcss": "^8.4.31",
    "postcss-pxtorem": "^6.0.0",
    "request": "^2.88.2",
    "typescript": "^5.3.2",
    "unplugin-auto-import": "^0.17.1",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^5.0.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-resolve": "^2.5.1",
    "vue": "^3.3.9",
    "vue-tsc": "^1.8.24",
    "orillusion-editor": "file:./packages/orillusion-editor"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 1234
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "dependencies": {
    "amfe-flexible": "^2.2.1",
    "axios": "^1.6.2",
    "element-plus": "^2.2.22",
    "js-cookie": "^3.0.1",
    "mitt": "^3.0.1",
    "pinia": "^2.1.7",
    "sass": "^1.69.5",
    "vant": "^4.8.0",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5"
  }
